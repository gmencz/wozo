{
  "private": true,
  "name": "wozo",
  "scripts": {
    "postinstall": "npm run build",
    "prebuild": "rm -rf build public/build",
    "build": "remix build",
    "dev": "remix run",
    "start": "remix-serve build",
    "css:watch": "postcss styles --base styles --dir app/styles -w",
    "css:build": "postcss styles --base styles --dir app/styles --env production",
    "prisma:migrate": "docker-compose exec app bash -c \"npx prisma migrate dev\"",
    "prisma:reset": "docker-compose exec app bash -c \"npx prisma migrate reset\""
  },
  "dependencies": {
    "@heroicons/react": "^1.0.3",
    "@prisma/client": "^2.29.0",
    "@remix-run/node": "^0.17.5",
    "@remix-run/react": "^0.17.5",
    "@remix-run/serve": "^0.17.5",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "^0.17.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.17.5",
    "@tailwindcss/forms": "^0.3.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "prisma": "^2.29.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
